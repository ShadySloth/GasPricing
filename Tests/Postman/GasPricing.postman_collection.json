		{
			"name": "Membership",
			"item": [
				{
					"name": "Calculate gas price regular",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var liters = 50;\r",
									"var gasType = \"Regular\";\r",
									"var membership = true;\r",
									"var expectedPrice = (liters * 12.70) * 0.85;\r",
									"\r",
									"pm.environment.set(\"liters\", liters);\r",
									"pm.environment.set(\"gasType\", gasType);\r",
									"pm.environment.set(\"membership\", membership);\r",
									"pm.environment.set(\"expectedPrice\", expectedPrice);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"let expectedPrice = pm.environment.get(\"expectedPrice\");\r",
									"\r",
									"pm.test(\"Regular gas price is correct with membership\", function(){\r",
									"    pm.response.to.have.status(200);\r",
									"    pm.expect(JSON.parse(pm.response.text()).totalPrice).to.equal(expectedPrice);\r",
									"})"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
